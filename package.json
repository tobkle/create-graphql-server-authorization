{
  "name": "create-graphql-server-authorization",
  "version": "0.0.5",
  "description": "Authorization package for create-graphql-server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "babel-node src/index.js",
    "start": "node dist/index.js",
    "eslint": "node_modules/.bin/eslint src",
    "prettier": "node_modules/.bin/prettier src/**/*.js --write",
    "pretest": "npm run build",
    "test": "mocha --compilers js:babel-core/register --require babel-polyfill --colors --reporter spec test/*",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register --require babel-polyfill --colors --reporter spec test/*",
    "esdoc": "node_modules/.bin/esdoc -c esdoc.json",
    "clean": "rm -rf dist/*",
    "build": "npm run clean && babel src -D --out-dir dist/",
    "prepublish": "npm run build"
  },
  "config": {
    "logfile": "log/all-logs-readable.log"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tobkle/create-graphql-server-authorization.git"
  },
  "keywords": [
    "create-graphql-server",
    "authorization"
  ],
  "author": "Tobias Klemmer <tobias@klemmer.info> (https://www.klemmer.info)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tobkle/create-graphql-server-authorization/issues"
  },
  "homepage": "https://github.com/tobkle/create-graphql-server-authorization#readme",
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "6.17.0",
    "babel-register": "6.16.3",
    "bson-objectid": "^1.1.5",
    "chai": "^4.0.1",
    "chai-fs": "^1.0.0",
    "dataloader": "^1.3.0",
    "esdoc": "^1.0.1",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "3.8.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-react-app": "^2.0.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "6.4.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "mongo-mock": "^2.4.1",
    "prettier": "^1.5.3"
  },
  "dependencies": {
    "lodash": "4.16.4",
    "mongodb": "^2.2.31",
    "winston": "^2.3.1"
  }
}
